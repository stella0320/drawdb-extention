{
    "manifest_version": 3,
    "name": "Drawdb.io Extension",
    "version": "1.0",
    "content_security_policy": {
        "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self';"
     },
    "icons": {
      "16": "favicon.ico",
      "48": "favicon.ico",
      "128": "favicon.ico"
    },
    // "action": {
    //   "default_popup": "dist/popup.html"
    // },
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_icon": {
        "16": "favicon.ico",
        "48": "favicon.ico",
        "128": "favicon.ico"
      },
      "default_title": "Open a new tab"
    },
    "permissions": ["tabs"]
  }